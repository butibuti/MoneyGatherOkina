private var isBattleUpdate:int;


function main():void{
	if(isBattleUpdate){
		
		EventExecute("Update_Battle");
	}
}

function OnStart():void{
	AddEventMessanger("Update_Battle");
	isBattleUpdate=true;
}

function OnBattleUpdateStop():void{
	isBattleUpdate=false;
}

function OnBattleUpdatetart():void{
	isBattleUpdate=true;
}