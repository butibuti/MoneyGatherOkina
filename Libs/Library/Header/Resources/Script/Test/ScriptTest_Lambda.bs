
public var speed:float;
private var moveFunction:()=>void;

function main():int
{
	if(TriggerKeyBoard(Keys.D)){
		moveFunction=()=>void{
			var velocity:Vector3;
			velocity.x=speed;
			Translate(velocity);
		};
	}
	if(TriggerKeyBoard(Keys.A)){
		moveFunction=()=>void{
			var velocity:Vector3;
			velocity.x=-speed;
			Translate(velocity);
		};
	}
	if(TriggerKeyBoard(Keys.W)){
		moveFunction=()=>void{
			var velocity:Vector3;
			velocity.z=speed;
			Translate(velocity);
		};
	}
	if(TriggerKeyBoard(Keys.S)){
		moveFunction=()=>void{
			var velocity:Vector3;
			velocity.z=-speed;
			Translate(velocity);
		};
	}
	
	moveFunction();
	return 0;
}

function OnStart():void
{
	var i:int;
	i=-i;
	moveFunction=()=>void{
		print("no move");
	};
}

