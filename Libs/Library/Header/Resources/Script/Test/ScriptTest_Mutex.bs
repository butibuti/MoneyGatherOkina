
public var count:int;


function main():int
{
	if(TriggerKeyBoard(Keys.Space)){
		print("count :"+ToString(count));
	}
	return 0;
}


function OnStart():void{
	PushTask("IncreaseCount");
	PushTask("IncreaseCount");
}

function IncreaseCount():void{
    var i:int;
	for(i=0;i<50;i++){
        count++;
	}

    print("Count UP!");
}